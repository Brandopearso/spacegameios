# Uncomment the next line to define a global platform for your project
 platform :ios, '9.0'

# Comment the next line if you're not using Swift and don't want to use dynamic frameworks

target 'SpaceGame' do
    
  use_frameworks!
  pod 'Firebase/Core'
  pod 'Firebase/Database'
  pod 'Firebase/Auth'
  
  target 'SpaceGameTests' do
    inherit! :search_paths
    pod 'Firebase/Core'
    pod 'Firebase/Database'
    pod 'Firebase/Auth'
    # Pods for testing
  end

  target 'SpaceGameUITests' do
    inherit! :search_paths
    pod 'Firebase/Core'
    
    pod 'Firebase/Database'
    pod 'Firebase/Auth'
    # Pods for testing
  end

end
